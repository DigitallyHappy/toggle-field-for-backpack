{
    "name": "backpack/addon",
    "description": ":package_description",
    "license": "license",
    "authors": [
        {
            "name": "author name",
            "email": "author email",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/backpack/addon",
    "keywords": ["Laravel", "addon"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "backpack\\addon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "backpack\\addon\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "backpack\\addon\\addonServiceProvider"
            ],
            "aliases": {
                "addon": "backpack\\addon\\Facades\\addon"
            }
        }
    }
}
